{
  "manifest_version": 3,
  "name": "Tab Commander",
  "version": "1.0",
  "description": "Fullscreen overlay showing tab titles, duplicates, and search",
  "permissions": ["tabs", "scripting", "activeTab", "windows"],
  "action": {
    "default_title": "Open Tab Overlay"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "host_permissions": ["<all_urls>"],
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-commander@example.com",
      "strict_min_version": "109.0"
    }
  }
}


